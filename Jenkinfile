pipeline { 
  agent any
  stages {
    stage ('Build') {
      steps {
        sh 'printenv'
        sh 'docker build -t nilotpal123/jenkins:""$BUILD_ID"" .'
      }
    }
     stage ('Publish') {
      steps {
        withDockerRegistry([credentialsID: "nilotpal123", url: ""]) {
        sh 'docker push nilotpal123/jenkins:""$BUILD_ID""'
        }
      } 
    }
  }
}
